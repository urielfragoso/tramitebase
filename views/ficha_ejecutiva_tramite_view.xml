<?xml version="1.0"?>
<odoo>
    <record id="ficha_tramite_form_view" model="ir.ui.view">
        <field name="name">ficha.tramite.form.view</field><!--NOMBRE DE LA VISTA -->
        <field name="model">sol.fichaejecutiva</field><!-- UBICACION Y NOMBRE DEL MODULO -->

        <field name="arch" type="xml">
            <!-- TIPO DE VISTA FORM-->
            <form string="Ficha Ejecutiva del tramite" id="Ficha_ejecutiva">

                <sheet>
                    <h2>Ficha Ejecutiva de la solicitud</h2>
                    <group>

                        <group>
                            <field name="observaciones"/>
                        </group>
                    </group>
                    <hr/>
                    <notebook>
                        <sheet string="Objeto asistencial">
                            <field name="objetoasistencial"/>

                        </sheet>

                    </notebook>


                </sheet>


            </form>
        </field>

    </record>

</odoo>